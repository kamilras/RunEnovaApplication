<Page x:Class="RunEnova.UstawieniaPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RunEnova"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UstawieniaPage">

    <Grid>
        <ComboBox x:Name="OperatorComBox" HorizontalAlignment="Left" Margin="27,44,0,0" VerticalAlignment="Top" Width="231" Height="35" DropDownOpened="OperatorComBox_DropDownOpened" SelectionChanged="OperatorComBox_SelectionChanged" />
        <Button x:Name="ResetPassBtn" Content="Zresetuj hasło" HorizontalAlignment="Left" Margin="277,44,0,0" VerticalAlignment="Top" Height="35" Width="163" Click="ResetPassBtn_Click"/>
        <Button x:Name="DemoBtn" Content="Ustaw licencję demo" HorizontalAlignment="Left" Margin="277,113,0,0" VerticalAlignment="Top" Height="35" Width="163" Click="DemoBtn_Click"/>

    </Grid>
</Page>
