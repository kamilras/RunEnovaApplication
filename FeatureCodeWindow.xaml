<Window x:Class="RunEnova.FeatureCodeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RunEnova"
        mc:Ignorable="d"
        Title="FeatureCodeWindow" Height="450" Width="800" Closed="Window_Closed">
    <Grid>
        <TextBox x:Name="CodeTxtBox" HorizontalAlignment="Stretch" TextWrapping="Wrap" VerticalAlignment="Stretch" />
    </Grid>
</Window>
